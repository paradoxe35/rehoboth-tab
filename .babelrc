{
    "presets": [
        "@babel/env"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "h",
                "pragmaFrag": "Fragment"
            }
        ],
        [
            "babel-plugin-jsx-pragmatic",
            {
                "module": "preact",
                "import": "h, Fragment",
                "export": "h, Fragment"
            }
        ]
    ]
}