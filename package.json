{
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm:server\" \"npm:browser-sync\" \"npm:vite:assets\"",
        "server": "cross-env DEV_MODE=true php artisan serve --host=0.0.0.0",
        "vite": "vite --port 8080",
        "vite:assets": "npm run assets-link && vite --port 8080",
        "assets-link": "symlink-dir ./assets ./public/dev",
        "browser-sync": "browser-sync start --proxy \"http://localhost:8000\"  --files \"resources/**/*.blade.php\" --no-notify --no-open --no-ui",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
